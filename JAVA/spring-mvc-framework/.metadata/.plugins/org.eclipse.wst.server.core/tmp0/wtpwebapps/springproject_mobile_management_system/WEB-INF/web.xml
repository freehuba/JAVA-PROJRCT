  	<display-name>DeleteMobile</display-name>
  	<description></description>
  	<servlet-class>com.jsp.mobile.components.DeleteMobile</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddMobile</servlet-name>
  	<url-pattern>/AddMobile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayAll</servlet-name>
  	<url-pattern>/DisplayAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateMobile</servlet-name>
  	<url-pattern>/UpdateMobile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
