  <servlet-class>com.jsp.servlet.Add</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>abcde</servlet-name>
  <servlet-class>com.jsp.servlet.Update</servlet-class>
  </servlet>
  
