  <servlet>
  <servlet-name>abcdfg</servlet-name>
  <servlet-class>com.jsp.servlet.Display</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>abcdfg</servlet-name>
  <url-pattern>/display</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>abcdf</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>abcde</servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>abcdd</servlet-name>
  <url-pattern>/student</url-pattern>
  </servlet-mapping>
  
